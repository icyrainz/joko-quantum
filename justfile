# JokoQuantum development commands

# Start dev server with HMR (auto-refreshes on file changes)
dev:
    npm run dev

# Run tests once
test:
    npm run test

# Run tests in watch mode
test-watch:
    npm run test:watch

# Lint
lint:
    npm run lint

# Production build
build:
    npm run build

# Preview production build locally
preview:
    npm run preview
